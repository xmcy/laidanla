.indexcontener{
  width: 100%;
  height: 9rem;
  position: relative;
  background-image: url(https://cdn.adquan.net/images/wx/mybackpic.png);
  background-size: 100% 100%;
  opacity: .9;
  border-bottom: .4rem solid #d8d7d7;
}
.myinfo{
  position: absolute;
  top: 2.7rem;
  left: 10%;
  width: 2.6rem;
  height: 2.6rem;
}
.mypic{
  width: 2.8rem;
  height: 2.8rem;
  border-radius: 1.4rem;
  background-color: #fff;
}
.shopname{
  position: absolute;
  top: 2.5rem;
  left: 27%;
  width: 72%;
  height: 1.8rem;
  line-height: 1.8rem;
  color: #fff;
  overflow: hidden;
  text-overflow: ellipsis;
  word-spacing: normal;
  font-size: 1.1rem;
}
.account{
  position: absolute;
  top: 4.5rem;
  left: 27%;
  width: 72%;
  height: 1rem;
  color: #fff;
  font-size: .8rem;
}
.account view{
  float: left;
  width: 50%;
}
.content{
  width: 96%;
  margin-left: 4%;
  height: 10rem;
}
.black{
  width: 100%;
  height: 15rem;
  background-color: #d8d7d7;
}
.content navigator{
  width: 100%;
  height: 3.3rem;
  border-bottom: 1px solid gainsboro;
}
.content navigator image{
  float: left;
  width: 1.3rem;
  height: 1.3rem;
  margin-top: 1rem;
}

.content navigator span{
  float: left;
  width: 78%;
  margin-left: 2%;
  height: 1.3rem;
  margin-top: 1.1rem;
}
.littlep{
  margin-right: .4rem;
}